{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "types": ["jest"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "src/**/*",
    "Testes/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "jest.config.js"
  ]
}
