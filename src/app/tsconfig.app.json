{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "baseUrl": ".",
    "types": ["jest"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
"include": [
  "src",
  "src/**/*",
  "Testes",
  "Testes/**/*",
  "**/*.test.ts",
  "**/*.test.tsx",
  ".expo/types/**/*.ts",
  "expo-env.d.ts",
  "jest.config.js"
]
}